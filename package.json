{
  "name": "simple-react-boilerplate",
  "version": "1.0.0",
  "description": "A simple react typescript starter kit",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack && cp src/index.html dist/index.html",
    "start": "./node_modules/.bin/webpack-dev-server --port 9000 --open --hot --inline --content-base src",
    "lint": "tslint 'src/**/*.ts{,x}'",
    "lint:fix": "tslint 'src/**/*.ts{,x}' --fix"
  },
  "engines": {
    "node": ">=v10.5.0"
  },
  "author": "ed@acutulus.co",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/styles": "^4.9.6",
    "axios": "^0.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "tslint": "^5.20.1",
    "tslint-defocus": "^2.0.6",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.5.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.0"
  }
}
